  <%= f.label :name %><br />
  <%= f.text_field :name %><br />
  </p>

  <p>
  <%= f.label :number %><br />
  <%= f.text_field :number %>
  </p>

  <p>
  <%= f.label :website %><br />
  <%= f.text_field :website %>
  </p>

  <p>
  <%= f.label :email %><br />
  <%= f.text_field :email %>
  </p>
  <%= f.submit %>

<% end %>
